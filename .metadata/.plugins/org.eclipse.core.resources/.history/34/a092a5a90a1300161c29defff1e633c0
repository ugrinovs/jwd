package domaci.zad3;


public class Utakmica {
	protected String klub1;
	protected String klub2;
	protected String datum;
	protected String vreme;
	
	public Utakmica(String klub1, String klub2, String datum, String vreme) {
		this.klub1 = klub1;
		this.klub2 = klub2;
		this.datum = datum;
		this.vreme = vreme;
	}
	
	public Utakmica(){
		
	}
	
	public String vratiTekstualnuReprezentacijuZaIspis(){
		return datum + " u " + vreme + " casova. Igrace " + klub1 + " protiv " + klub2;
	}
	
	
}
